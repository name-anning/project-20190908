.input{
  display: flex;
  flex-direction: row;
  width: 96%;
  height: 25px;
  background: rgb(235, 232, 232);
  border-radius: 10px;
  margin: 2%;
}
.input image{
  width: 20px;
  height: 20px;
  margin: 2px 10px;
}
.icon{
  position: absolute;
  top: -20px;
}
.swiper{
  width: 100%;
  height: 500px;
}
.swiper image{
  width: 100%;
  height: 500px;
}
.nav{
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  text-align: center;
  margin: 10px 0;
}
.nav_icon{
  display: flex;
  flex-direction: column;
}
.nav_icon text{
  font-size: 12px;
  color: #000;
}
.image{
  height: auto;
}
.image image{
  width: 100%;
  height: auto;
  margin: 0;
}
.swiper2{
  height: 250px;
}
.swiper2 image{
  width: 100%;
}
.imglist image{
  width: 100%;
}